{
  "name": "grainular-examples",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Randy Uebel <randy.uebel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean:dist",
    "build": "react-scripts build",
    "clean:dist": "rm -rf ./docs",
    "copy:docs": "cp docs/src/.gitignore docs/dist/.gitignore",
    "lint": "esw -f simple-detail --color src",
    "lint:watch": "npm run lint -- --watch",
    "publish": "build && publish:docs",
    "publish:docs": "git subtree push --prefix docs/dist origin gh-pages",
    "start": "react-scripts start"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browser": true
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "gsandf-react",
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-watch": "^2.0.5",
    "eslint": "^4.6.1",
    "eslint-config-gsandf-react": "^0.2.0",
    "eslint-watch": "^3.1.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "^1.0.13",
    "styled-components": "^2.1.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  }
}
