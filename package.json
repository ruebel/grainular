{
  "name": "grainular",
  "version": "0.1.0",
  "main": "lib/index.js",
  "author": "Randy Uebel <randy.uebel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean:dist",
    "build": "babel ./src -d ./lib",
    "build:docs": "cd ./examples && npm run build",
    "clean:dist": "rm -rf ./lib",
    "lint": "esw -f simple-detail --color src",
    "lint:watch": "npm run lint -- --watch",
    "postinstall": "cd ./examples && yarn install",
    "start": "cd ./examples && npm run start"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browser": true
          }
        }
      ],
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "gsandf-react",
    "parser": "babel-eslint"
  },
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0",
    "react-dom": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-watch": "^2.0.5",
    "eslint": "^4.6.1",
    "eslint-config-gsandf-react": "^0.2.0",
    "eslint-watch": "^3.1.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  }
}
